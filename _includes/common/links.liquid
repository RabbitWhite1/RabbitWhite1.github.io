{%- comment %} dns-prefetch {% endcomment %}
<link rel="dns-prefetch" href="https://cdn.jsdelivr.net">

{%- comment %} canonical link {% endcomment -%}
{%- if prev -%}
    <link rel="prev" href="{{ prev.url | absolute_url }}">
{%- endif -%}

{%- if next -%}
    <link rel="next" href="{{ next.url | absolute_url }}">
{%- endif -%}

<link rel="canonical" href="{{ schema_surl }}">

{%- comment %} favicon icon {% endcomment -%}
<link rel="icon" type="image/png" href="{{ site.baseurl }}/assets/images/favicon.png">
{%- comment %} alternate {% endcomment -%}
{% comment %} <link rel="icon" type="image/png" href="{{ site.baseurl }}/assets/images/favicon-16x16.png" sizes="16x16"> {% endcomment %}
{% comment %} <link rel="icon" type="image/png" href="{{ site.baseurl }}/assets/images/favicon-32x32.png" sizes="32x32"> {% endcomment %}
{% comment %} <link rel="icon" type="image/png" href="{{ site.baseurl }}/assets/images/favicon-96x96.png" sizes="96x96"> {% endcomment %}

{%- comment %} safari {% endcomment -%}
{% comment %} <link rel="mask-icon" href="{{ site.baseurl }}/assets/images/favicon.svg" color="{{ theme_color }}"> {% endcomment %}

{%- comment %} apple-touch-icon {% endcomment -%}
{% comment %} <link rel="apple-touch-icon" href="{{ site.baseurl }}/assets/images/apple-touch-icon-300x300.jpg"> {% endcomment %}
